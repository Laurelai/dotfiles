set nocompatible
syntax on
filetype on
filetype indent on
filetype plugin on
autocmd FileType ruby,eruby,yaml,cucumber set autoindent shiftwidth=2 softtabstop=2 tabstop=2 expandtab
autocmd FileType scheme,lisp set autoindent shiftwidth=2 softtabstop=2 tabstop=2 expandtab
colorscheme desert 
let b:is_chicken=1
